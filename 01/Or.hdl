/**
 * Or gate:
 * if ((a == 1) or (b == 1)) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Nand (a=a, b=a, out=n1out);
    Nand (a=b, b=b, out=n2out);
    Nand (a=n1out, b=n2out, out=out);
}
